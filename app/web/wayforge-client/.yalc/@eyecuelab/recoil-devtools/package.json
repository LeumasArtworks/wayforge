    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/recoil-devtools.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/recoil-devtools.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "recoil": ">=0.7.6"
  },
  "dependencies": {
    "framer-motion": "^7.3.2",
