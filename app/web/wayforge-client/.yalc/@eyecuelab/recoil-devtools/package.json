  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eyecuelab/recoil-devtools.git"
  },
  "keywords": [
    "recoil",
    "devtools",
    "react",
    "state",
    "atom",
    "selector"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
